    ViewBag.Title = "Delete";
}
<br />
<br />

<div class="row">
    <div class="col-md-6">
        <h3>Eliminar Pregunta</h3>
        <hr />
    </div>
</div>

<div>

    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Examen.NombreExamen)
