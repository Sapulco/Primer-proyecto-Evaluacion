@model IEnumerable<Evaluacion.Models.TipoPregunta>

@{
    ViewBag.Title = "Index";
}
<br />
<br />
<h2>Tipos de Pregunta</h2>
<br />
<p>
    @Html.ActionLink("Crear Nuevo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsListado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsMultiple)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsListado)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsMultiple)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
